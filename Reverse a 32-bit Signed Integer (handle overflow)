#include <stdio.h>
#include <limits.h>

int reverse(int x) {
    long long rev = 0;
    int n = x;
    while (n != 0) {
        rev = rev * 10 + n % 10;
        n /= 10;
        if (rev > INT_MAX || rev < INT_MIN) return 0;
    }
    return (int)rev;
}

int main() {
    int x = 123;
    printf("Original: %d, Reversed: %d\n", x, reverse(x));
    return 0;
}
