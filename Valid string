#include <stdio.h>
#include <ctype.h>

int isValid(const char *s) {
    while (*s) {
        if (!isalpha((unsigned char)*s)) return 0;
        s++;
    }
    return 1;
}

int main() {
    char str[100];
    printf("Enter string: ");
    scanf("%99s", str);
    printf("Valid? %s\n", isValid(str) ? "Yes" : "No");
    return 0;
}
