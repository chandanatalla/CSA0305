#include <stdio.h>

int find(int arr[], int n, int key) {
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) return i;
    }
    return -1;
}

int main() {
    int arr[] = {10, 20, 30, 40};
    int key = 30;
    int pos = find(arr, 4, key);
    printf(pos >= 0 ? "Found at %d\n" : "Not found\n", pos);
    return 0;
}
